{
  "name": "parzival42codes/laravel-aire-bootstrap5-theme",
  "description": "laravel Aire Bootstrap5 Theme",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^5.0|^6.0|^7.0",
    "spatie/laravel-package-tools": "^1.9",
    "glhd/aire": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "parzival42codes\\LaravelAireBootstrap5Theme\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "parzival42codes\\LaravelAireBootstrap5Theme\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "parzival42codes\\LaravelAireBootstrap5Theme\\LaravelAireBootstrap5ThemeServiceProvider"
      ]
    }
  }
}
